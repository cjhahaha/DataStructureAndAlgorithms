#define PRON "loop_table"
#include <cstdio>
#include <vector>
#include <string>
#include <cstring>
#include <iomanip>
#include <iostream>
#include <algorithm>
using namespace std;
typedef long long ll;

const int MAXN = 100 + 10;

int n, res[MAXN][MAXN];


void schedule(int n) {
	// init
	memset(res, 0, sizeof res);
	for (int i = 1; i <= n; i ++)
		res[1][i] = res[i][1] = i;

	for (int gap = 1, lg = 0; gap < n; gap *= 2, lg ++) 
		for (int t = 0; t < n / gap; t ++) {
			for (int i = gap; i <= gap * 2; i ++)
				for (int j = gap; j <= gap * 2; j ++)
					res[i][j + lg * gap * 2]	   = res[i - m][j + lg * gap * 2 - gap],
					res[i][j + lg * gap * 2 - gap] = res[i - m][j + lg * gap * 2];
		}
}


void output() {
	cout << "Result:";
	for (int i = 1; i <= n; i ++) {
		cout << endl << res[i][1] << "  |  ";
		for (int j = 2; j <= n; j ++)
			cout << setw(4) << setfill(' ') << res[i][j] << " ";
	}
}


int main() {
	// input
	// cin >> n;
	n = 8;

	schedule(n);
	output();
}
